{
  "manifest_version": 3,
  "name": "Close Tabs and Store URLs",
  "version": "1.0",
  "permissions": ["tabs", "storage", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Close Tabs"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
